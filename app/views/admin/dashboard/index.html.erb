<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2 class='mt-4'>New Users</h2>
      <div class="table-responsive-sm">
        <table class="table table-bordered table-striped">
        	<thead>
        		<th>First Name</th>
        		<th>Last Name</th>
        		<th>Email</th>
        		<td>Timezone</td>
        		<th>Member since</th>
        		<th>Action</th>
        	</thead>
        	<tbody>
      			<% @users.present? && @users.each do |user| %>
      				<tr>
      					<td><%= user.first_name %></td>
      					<td><%= user.last_name %></td>
      					<td><%= user.email %></td>
      					<td><%= user.timezone %></td>
      					<td><%= user.created_at.strftime("%Y-%m-%d") %></td>
      					<td><%= link_to edit_admin_user_path(id: user.id), class: 'btn btn-prmary btn-sm' do %><i class="fa fa-edit"></i><% end %></td>
      				</tr>
      			<% end %>
        	</tbody>
       </table>      
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <h2 class='mt-4'>New Users</h2>
      <% @users.each do |user| %>
        <% user.email %>
        <% user.tasks.each do |task| %>
          <% task.status == true %>
          <% task.title %>
        <% end %>  
      <% end %>      
    </div>
  </div>  
</div>
