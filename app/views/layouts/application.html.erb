<!DOCTYPE html>
<html>
  <head>
    <title>Slack test</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <!--   <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script> -->

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js"></script> -->  <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.9/summernote-lite.js"></script> -->
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  </head>

  <body>
    <header class="p-3 px-md-4 mb-5 header-bg border-bottom box-shadow">
      <div class="container">
        <%= render partial: 'shared/header' %>
      </div>
    </header>
    <div class="container container-height">
      <div class="row">
      <div class="col-md-12">
        <div id="message_container">
          <%= render partial: 'shared/messages' %>
        </div>
      <%# if flash[:notice].present? %>
        <!-- <div class="alert alert-success mt-3">
          <div class="notice"><%= notice %></div>
        </div> -->
      <%# elsif flash[:error].present? %>
        <!-- <div class="alert alert-danger mt-3">
          <div class="alert"><%= alert %></div>
        </div>  --> 
      <%# end %>
      </div>
      </div>
      <%= yield %>
    </div>
    <footer class="mt-5">
      <%= render partial: 'shared/footer' %>
    </footer>
    <div class="d-none" id="page-loader">
      <h3><i class="fa fa-circle-o-notch fa-spin fa-2x"></i></h3>
    </div>
  </body>
</html>
